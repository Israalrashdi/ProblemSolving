      "source": [
        "h = int(input(\"Enter Height: \"))\n",
        "for i in range(h):\n",
        "   print('  ' * (h - i - 1), end='')\n",
        "# The Left side\n",
        "   x = 1\n",
        "   for j in range(i + 1):\n",
        "       print(x, end=' ')\n",
        "       x *= 2\n",
        "# The Right side\n",
        "   x //= 4\n",
        "   for j in range(i):\n",
        "       print(x, end=' ')\n",
        "       x //= 2\n",
        "   print()"
      ]
    }
  ]
}